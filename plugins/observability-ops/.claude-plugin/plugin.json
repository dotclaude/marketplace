{
  "name": "observability-ops",
  "displayName": "Observability & Operations Excellence",
  "version": "1.0.0",
  "description": "Production reliability and observability across all environments. Master Datadog, CloudWatch, monitoring, incident response, SRE practices, and audit logging for enterprise compliance.",
  "author": {
    "name": "DotClaude",
    "url": "https://github.com/dotclaude"
  },
  "repository": "https://github.com/dotclaude/marketplace",
  "license": "MIT",
  "keywords": [
    "monitoring",
    "observability",
    "datadog",
    "cloudwatch",
    "incident-response",
    "auditing",
    "compliance",
    "sre",
    "reliability"
  ],
  "category": "development",
  "components": {
    "commands": [
      "monitor",
      "trace",
      "incident",
      "slo",
      "audit"
    ],
    "agents": [
      "datadog-specialist",
      "cloudwatch-expert",
      "sre-engineer",
      "performance-analyst",
      "log-aggregator",
      "compliance-auditor"
